{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": [
    "env.d.ts",
    "declarations.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
